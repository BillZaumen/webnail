<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/prs.wtz.webnail-template+xml">
    <comment>Webnail Input File</comment>
    <sub-class-of type='application/xml; charset="utf-8"'/>
    <magic priority="80">
      <match type="string" offset="0"
	     value="(!M.T application/prs.wtz.webnail-template+xml)"/>
      <!-- match when a byte order mark is present -->
      <match type="big32" value="0xefbbbf3c" offset="0">
	<match type="string" offset="3" 
	       value="(!M.T application/prs.wtz.webnail-template+xml)"/>
      </match>
    </magic>
    <glob pattern="*.wnlt"/>  
    <glob pattern="*.xml"/>  
  </mime-type>
</mime-info>
