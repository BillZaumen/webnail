<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/prs.wtz.webnail-layout+xml">
    <comment>Webnail Input File</comment>
    <sub-class-of type='application/xml; charset="utf-8"'/>
    <magic priority="80">
      <match type="string" offset="0" value="&lt;?xml ">
	<match type="string" offset="16:96"
	       value="&lt;?M.T application/prs.wtz.webnail-layout+xml?&gt;"/>
      </match>
      <!-- match when a byte order mark is present -->
      <match type="big32" value="0xefbbbf3c" offset="0">
	<match type="string" offset="3" value="&lt;?xml ">
	  <match type="string" offset="19:99"
		 value="&lt;?M.T application/prs.wtz.webnail-layout+xml?&gt;"/>
	</match>
      </match>
    </magic>
    <glob pattern="*.wlo"/>
    <glob pattern="*.xml"/>
  </mime-type>
</mime-info>
